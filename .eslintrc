{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "globals": {
    "APP_DEFAULT_LOCALE": false,
    "APP_DEV": false,
    "APP_TARGET_DEVICE": false
  },
  "rules": {
    "import/prefer-default-export": "off",
    "react/no-find-dom-node": "off",
    "react/no-multi-comp": "off",
    "react/prefer-stateless-function": 0,
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["mock-server/**/*", "**/*.test.js", "**/*.spec.js"]}],
    "jsx-a11y/media-has-caption": 0,
    "no-param-reassign": ["error", { "props": false }]
  }
}