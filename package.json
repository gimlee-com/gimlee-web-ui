{
  "name": "gimlee-web-ui",
  "version": "1.0.0",
  "description": "gimlee web front-end",
  "main": "index.js",
  "config": {
    "BRAND": "default",
    "APP": "hello"
  },
  "scripts": {
    "build": "npm run clean && concurrently \"npm run build:desktop\" \"npm run build:mobile\"",
    "build:desktop": "cross-env NODE_ENV=production TARGET_DEVICE=desktop webpack -p",
    "build:mobile": "cross-env NODE_ENV=production TARGET_DEVICE=mobile webpack -p",
    "buildAndAnalyzeBundle": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "brand:gimlee-default": "npm config set gimlee-web-ui:BRAND gimlee-default",
    "brand:default": "npm config set gimlee-web-ui:BRAND default",
    "app:gimlee": "npm config set gimlee-web-ui:APP gimlee",
    "app:hello": "npm config set gimlee-web-ui:APP hello",
    "start": "cross-env parallelshell \"npm run start:mock-server\" \"npm run start:desktop\" \"npm run start:mobile\"",
    "start:mock-server": "nodemon --watch mock-server mock-server/index.js",
    "start:desktop": "cross-env TARGET_DEVICE=desktop BRAND=${npm_package_config_BRAND} APP=${npm_package_config_APP} webpack-dev-server --port 8080 --host 0.0.0.0",
    "start:mobile": "cross-env TARGET_DEVICE=mobile BRAND=${npm_package_config_BRAND} APP=${npm_package_config_APP} webpack-dev-server --port 8181 --host 0.0.0.0",
    "startAndAnalyzeBundle": "cross-env ANALYZE=true npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "gimlee",
  "license": "SEE LICENSE AT THE ROOT OF THE PROJECT",
  "dependencies": {
    "axios": "0.18.0",
    "classnames": "2.2.5",
    "commonmark": "0.28.1",
    "cropperjs": "1.5.13",
    "debounce": "1.1.0",
    "document-offset": "1.0.4",
    "except": "0.1.3",
    "flatpickr": "3.1.5",
    "framer-motion": "1.11.0",
    "history": "4.7.2",
    "i18next": "11.9.0",
    "i18next-browser-languagedetector": "2.2.4",
    "js-cookie": "2.2.0",
    "jwt-decode": "2.2.0",
    "linkifyjs": "2.1.7",
    "localforage": "1.7.2",
    "lodash.groupby": "4.5.0",
    "lodash.isequal": "4.5.0",
    "moment": "2.22.1",
    "ol": "5.3.3",
    "password-validator": "4.1.1",
